<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="student_login" prefHeight="650.0" prefWidth="850.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.studentController">
   <children>
      <TextField fx:id="student_user" layoutX="262.0" layoutY="150.0" prefHeight="53.0" prefWidth="312.0" promptText="Username">
         <font>
            <Font size="24.0" />
         </font>
      </TextField>
      <PasswordField fx:id="student_pass" layoutX="261.0" layoutY="244.0" prefHeight="53.0" prefWidth="312.0" promptText="Password">
         <font>
            <Font size="24.0" />
         </font>
      </PasswordField>
      <Label fx:id="Studentlvl" alignment="CENTER" layoutX="311.0" layoutY="60.0" prefHeight="34.0" prefWidth="229.0" text="STUDENT">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <Button fx:id="student_loginbtn" layoutX="346.0" layoutY="352.0" mnemonicParsing="false" onAction="#login" prefHeight="35.0" prefWidth="147.0" text="Login" />
      <Button fx:id="back_from_slogin_btn" layoutX="346.0" layoutY="427.0" mnemonicParsing="false" onAction="#back_from_student_login" prefHeight="35.0" prefWidth="147.0" text="Back" />
   </children>
</AnchorPane>
