<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="teacher_panel" prefHeight="650.0" prefWidth="850.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.TeacherController">
   <children>
      <Label fx:id="teacherpanel_level" alignment="CENTER" contentDisplay="CENTER" layoutX="283.0" layoutY="55.0" prefHeight="36.0" prefWidth="286.0" text="TEACHER PANEL">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <AnchorPane fx:id="Teacherpanel_insidepane" layoutX="239.0" layoutY="150.0" prefHeight="269.0" prefWidth="373.0">
         <children>
            <Button fx:id="add_result" layoutX="50.0" layoutY="34.0" mnemonicParsing="false" onAction="#add_result" prefHeight="49.0" prefWidth="273.0" text="ADD RESULT">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Button>
            <Button fx:id="modify_result" layoutX="52.0" layoutY="110.0" mnemonicParsing="false" onAction="#modify_result" prefHeight="49.0" prefWidth="273.0" text="MODIFY RESULT">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Button>
            <Button fx:id="t_changepass_btn" layoutX="52.0" layoutY="185.0" mnemonicParsing="false" onAction="#changepass" prefHeight="49.0" prefWidth="273.0" style="-fx-background-color: #CB4335; -fx-text-fill: white;" text="CHANGE PASSWORD">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
      <Button fx:id="teacher_logout" layoutX="510.0" layoutY="501.0" mnemonicParsing="false" onAction="#logout" prefHeight="36.0" prefWidth="145.0" text="Logout" />
   </children>
</AnchorPane>
