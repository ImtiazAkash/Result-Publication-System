<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="adminroot" prefHeight="650.0" prefWidth="850.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.AdminController">
   <children>
      <TextField fx:id="Adminuser" layoutX="302.0" layoutY="144.0" prefHeight="53.0" prefWidth="238.0" promptText="Username">
         <font>
            <Font size="24.0" />
         </font>
      </TextField>
      <PasswordField fx:id="Adminpass" layoutX="302.0" layoutY="226.0" prefHeight="53.0" prefWidth="238.0" promptText="Password">
         <font>
            <Font size="24.0" />
         </font>
      </PasswordField>
      <Button fx:id="Admin_loginbtn" layoutX="320.0" layoutY="300.0" mnemonicParsing="false" onAction="#login" prefHeight="39.0" prefWidth="193.0" text="Login">
         <font>
            <Font size="18.0" />
         </font></Button>
      <Label fx:id="Adminlvl" alignment="CENTER" layoutX="311.0" layoutY="71.0" prefHeight="37.0" prefWidth="229.0" text="ADMIN">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <Button fx:id="back_front" layoutX="320.0" layoutY="378.0" mnemonicParsing="false" onAction="#back_from_admin_login" prefHeight="39.0" prefWidth="193.0" text="Back">
         <font>
            <Font size="18.0" />
         </font></Button>
   </children>
</AnchorPane>
