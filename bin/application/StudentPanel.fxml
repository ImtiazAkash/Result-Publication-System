<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="student_panel" prefHeight="650.0" prefWidth="850.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.studentController">
   <children>
      <Label fx:id="studentpanel_level" alignment="CENTER" contentDisplay="CENTER" layoutX="282.0" layoutY="73.0" prefHeight="36.0" prefWidth="286.0" text="STUDENT PANEL">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <AnchorPane fx:id="StudentPanel_insidepane" layoutX="252.0" layoutY="129.0" prefHeight="267.0" prefWidth="359.0" style="-fx-border-color: white;">
         <children>
            <Button fx:id="search_result" layoutX="79.0" layoutY="43.0" mnemonicParsing="false" onAction="#show_result" prefHeight="49.0" prefWidth="202.0" text="VIEW RESULT">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Button>
            <Button fx:id="change_pass_btn" layoutX="79.0" layoutY="159.0" mnemonicParsing="false" onAction="#stdchangepass" prefHeight="49.0" prefWidth="202.0" style="-fx-background-color: #CB4335; -fx-text-fill: white;" text="CHANGE PASSWORD">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
      <Button fx:id="student_logout" layoutX="364.0" layoutY="423.0" mnemonicParsing="false" onAction="#logout" prefHeight="36.0" prefWidth="148.0" text="Logout" />
   </children>
</AnchorPane>
